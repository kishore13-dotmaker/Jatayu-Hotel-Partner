{
  "expo": {
    "name": "phonestly",
    "slug": "phonestly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app_icon/applogo.png",
    "splash": {
      "image": "./src/assets/images/app_icon/applogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.initstate.phonestly"
    },
    "android": {  
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app_icon/applogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.initstate.phonestly"
    },
    "web": {
      "favicon": "./src/assets/images/app_icon/applogo.png"
    }
  }
}
